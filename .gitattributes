# Large binary artifacts go through Git LFS
*.pt filter=lfs diff=lfs merge=lfs -text
*.pth filter=lfs diff=lfs merge=lfs -text
*.pkl filter=lfs diff=lfs merge=lfs -text
*.bin filter=lfs diff=lfs merge=lfs -text
embeddings/** filter=lfs diff=lfs merge=lfs -text
models/model_epoch_15.pth filter=lfs diff=lfs merge=lfs -text
models/fclip_category_classifier.pth filter=lfs diff=lfs merge=lfs -text
